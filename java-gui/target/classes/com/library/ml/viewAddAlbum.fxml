<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.library.ml.ViewAddAlbumController">
   <children>
      <Text layoutX="82.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add album" />
      <TableView fx:id="songsTable" layoutX="82.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="songId" prefWidth="25.0" text="nr." />
          <TableColumn fx:id="songTitle" prefWidth="174.0" text="song title" />
        </columns>
      </TableView>
      <TableView fx:id="authorsTable" layoutX="330.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="authorId" prefWidth="23.0" text="nr." />
          <TableColumn fx:id="authorName" prefWidth="176.0" text="author" />
        </columns>
      </TableView>
      <Button fx:id="albumAddSongButton" layoutX="140.0" layoutY="361.0" mnemonicParsing="false" onAction="#albumAddSongButton" text="add song" />
      <TextField fx:id="songNameTextField" layoutX="108.0" layoutY="330.0" />
      <Button fx:id="albumAddAuthorButton" layoutX="393.0" layoutY="361.0" mnemonicParsing="false" onAction="#albumAddAuthorButton" text="add author" />
      <TextField fx:id="authorsNameTextField" layoutX="356.0" layoutY="330.0" />
      <Button fx:id="albumAddAlbumButton" layoutX="457.0" layoutY="52.0" mnemonicParsing="false" onAction="#albumAddAlbumButton" text="add album" />
      <Label layoutX="38.0" layoutY="334.0" text="song name:" />
      <Label layoutX="270.0" layoutY="334.0" text="authors name:" />
      <Button fx:id="albumRemoveSongButton" layoutX="196.0" layoutY="300.0" mnemonicParsing="false" onAction="#albumRemoveSongButton" text="remove song" />
      <Button fx:id="albumRemoveAuthorButton" layoutX="435.0" layoutY="300.0" mnemonicParsing="false" onAction="#albumRemoveAuthorButton" text="remove author" />
   </children>
</AnchorPane>
